<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.glr.populis" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>Populis</name>
    <description>
        Aplicativo do Populis Mobile
    </description>
    <author email="desenv@glr.com.br" href="http://www.glr.com.br">
        Equipe de Desenvolvimento - GLR
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
